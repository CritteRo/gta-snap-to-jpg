{
  "plugins": ["prettier"],
  "extends": ["react-app", "prettier"],
  "rules": {
    "arrow-body-style": ["error", "as-needed"],
    "no-unused-expressions": "error",
    "no-unused-labels": "error",
    "prefer-const": "error",
    "prefer-template": "error",
    "prettier/prettier": "error",
    "curly": "error",
    "no-unused-vars": "off",
    "@typescript-eslint/no-unused-vars": "error",
    "no-console": "warn"
  }
}
